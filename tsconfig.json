{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "strict": true,
    "baseUrl": "app",
    "module": "esnext",
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "sourceMap": true,
  }
}
